<h1>Editing software</h1>

<% form_for(@software) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :owner %><br />
    <%= f.text_field :owner %>
  </p>
  <p>
    <%= f.label :license_key %><br />
    <%= f.text_field :license_key %>
  </p>
  <p>
    <%= f.label :number_of_licenses %><br />
    <%= f.text_field :number_of_licenses %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @software %> |
<%= link_to 'Back', softwares_path %>
